{
  "name": "cyclop-graph",
  "version": "0.1.12",
  "description": "Graphql client lightweight query and mutation",
  "main": "lib/cyclop.js",
  "browser": "dist/cyclop.js",
  "unpkg": "dist/cyclop.js",
  "author": "kenshero",
  "repository": {
    "type": "git",
    "url": "git@github.com:kenshero/cyclop-graph.git"
  },
  "license": "MIT",
  "dependencies": {
    "isomorphic-unfetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "faucet": "^0.0.1",
    "istanbul": "^0.4.5",
    "np": "^2.13.2",
    "tape": "^4.6.3",
    "webpack": "^2.3.2"
  },
  "scripts": {
    "prepublish": "yarn run build && yarn run lint && yarn run test",
    "lint": "eslint src && eslint test",
    "test": "babel-tape-runner test/*.js | faucet",
    "build:commonjs": "babel src --out-dir lib",
    "build:umd": "webpack",
    "build:umd:min": "NODE_ENV=production webpack",
    "build": "yarn run build:commonjs && yarn run build:umd && yarn run build:umd:min",
    "release": "np",
    "coverage": "covert test/*.js"
  },
  "keywords": [
    "cyclop",
    "cyclop-graph",
    "graphql",
    "graphql-client"
  ],
  "bugs": {
    "url": "https://github.com/kenshero/cyclop-graph/issues"
  }
}
